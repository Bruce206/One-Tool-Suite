<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.effect.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import com.cathive.fonts.fontawesome.*?>
<?import com.cathive.fonts.fontawesome.FontAwesomeIconView?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.bruss.config.ConfigTableCtrl">
	<left>
		<TableView fx:id="configTable" prefHeight="600.0" prefWidth="501.0" style="-fx-font-size: 12px;" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="name" editable="false" maxWidth="-1.0" minWidth="220.0" prefWidth="220.0" text="Name" />
				<TableColumn fx:id="host" editable="false" maxWidth="-1.0" minWidth="200.0" prefWidth="325.0" text="Hostname / IP" />
				<TableColumn fx:id="springBootConfigColumn" prefWidth="20.0">
					<graphic>
						<FontAwesomeIconView icon="ICON_LEAF"></FontAwesomeIconView>
					</graphic>
				</TableColumn>
				<TableColumn fx:id="databaseConfigColumn" prefWidth="20.0">
					<graphic>
						<FontAwesomeIconView icon="ICON_REORDER"></FontAwesomeIconView>
					</graphic>
				</TableColumn>
				<TableColumn fx:id="fileSyncConfigColumn" prefWidth="20.0">
					<graphic>
						<FontAwesomeIconView icon="ICON_COPY"></FontAwesomeIconView>
					</graphic>
				</TableColumn>
			</columns>
		</TableView>
	</left>
	<center>
		<fx:include fx:id="editConfigTab" source="EditConfig.fxml" BorderPane.alignment="CENTER" />
	</center>
</BorderPane>
